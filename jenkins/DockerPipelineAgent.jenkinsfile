pipeline {
    agent {
        node {
            label "commons-docker-ncias-p2236-v"
        }
    }
    stages {
        stage('Test') {
            agent {
                docker{
                    image "ncidockerhub.nci.nih.gov/icdc/jenkins-agent-ansible"
                    registryUrl "https://ncidockerhub.nci.nih.gov"
                    registryCredentialsId "ncidockerhub"
                    args "--net=host -u root"
                    reuseNode true
                }
            }
            steps {
                sh 'ansible --version'
            }
        }
    }
}